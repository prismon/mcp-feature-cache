{
  "name": "mcp-feature-store",
  "version": "1.0.0",
  "description": "MCP Feature Store - A streamable service for feature extraction and storage",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch --no-stdin src/express-mcp-server.ts",
    "dev:background": "tsx src/express-mcp-server.ts",
    "start": "tsx src/express-mcp-server.ts",
    "test": "vitest",
    "test:smoke": "tsx test/smoke-test.ts",
    "test:smoke:help": "tsx test/smoke-test.ts --help",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src extractors --ext .ts",
    "db:init": "tsx src/db/init.ts",
    "logs": "tsx scripts/view-logs.ts",
    "logs:tail": "tsx scripts/view-logs.ts tail",
    "logs:errors": "tsx scripts/view-logs.ts errors --stack",
    "logs:verbose": "tsx scripts/view-logs.ts verbose"
  },
  "keywords": [
    "mcp",
    "feature-store",
    "extraction",
    "embeddings"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.2",
    "@types/express": "^5.0.3",
    "better-sqlite3": "^11.5.0",
    "cheerio": "^1.0.0",
    "express": "^5.1.0",
    "lru-cache": "^11.0.2",
    "marked": "^14.1.4",
    "mime-types": "^2.1.35",
    "node-html-parser": "^6.1.13",
    "openai": "^4.73.0",
    "p-limit": "^6.1.0",
    "sharp": "^0.33.5",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.17.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
